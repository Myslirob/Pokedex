{
  "name": "pokedex",
  "version": "1.0.0",
  "private": true,
  "author": "Robert Myslivecek",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "static": "serve -l 3000 out",
    "check": "yarn run check:cs && yarn run check:types",
    "check:cs": "eslint . --cache --cache-strategy content --cache-location .cache/.eslintcache --ext .js,.ts,.tsx,.json",
    "check:types": "tsc --incremental --tsBuildInfoFile '.cache/.ts.incremental.cache' --noEmit",
    "format": "eslint . --ext .js,.ts,.tsx,.json --fix",
    "codegen": "yarn graphql-codegen --config bin/codegen.ts",
    "codegen:watch": "yarn graphql-codegen --watch --config bin/codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tensorflow/tfjs": "^4.6.0",
    "assert-never": "^1.2.1",
    "canvas": "^2.11.2",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "graphql": "^16.6.0",
    "next": "13.4.2",
    "next-pwa": "^5.6.0",
    "notistack": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-webcam": "^7.1.0",
    "styled-components": "^6.0.0-rc.1",
    "use-sound": "^4.0.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@types/node": "20.1.4",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.27.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-quotes": "0.0.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "serve": "14.1.1",
    "typescript": "5.0.4"
  }
}
